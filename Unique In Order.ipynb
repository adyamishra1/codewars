   "source": [
    "def unique_in_order(iterable):\n",
    "    output = []\n",
    "    for i in range(0, len(iterable)):\n",
    "        if iterable[i:i+2].count(iterable[i]) == 1:\n",
    "            output.append(iterable[i])\n",
    "    return output"
   ]
  }
